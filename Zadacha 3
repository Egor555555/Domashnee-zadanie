#include "pt4.h"
using namespace std;

void Solve()
{
    Task("TheTwo23");
        int n, x, y;
        bool z = true;
        pt >> n;
        while (n > 10)
        {
            x = n % 10;
            n = n / 10;
            y = n % 10;
            if (y < x) {
                z = false;
                break;
            }
        }pt << z;
        
}
